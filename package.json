{
  "name": "postcss-pseudo-class-any-button",
  "description": "PostCSS plugin for targeting all button elements with :any-button selector",
  "version": "0.1.3",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "button",
    "pseudo",
    "element"
  ],
  "repository": "andrepolischuk/postcss-pseudo-class-any-button",
  "dependencies": {
    "postcss": "^5.0.21",
    "postcss-selector-parser": "^2.1.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chokidar-cli": "^1.1.0",
    "rimraf": "^2.5.2"
  },
  "main": "index.es5.js",
  "scripts": {
    "test": "ava --require babel-register",
    "tdd": "chokidar \"*.js\" --command \"npm run test\" --initial",
    "prepublish": "babel index.js --out-file index.es5.js",
    "postpublish": "rimraf index.es5.js && git push --follow-tags"
  },
  "engines": {
    "node": ">=0.12"
  },
  "author": "Andrey Polischuk <andre.polischuk@gmail.com>",
  "license": "MIT"
}
